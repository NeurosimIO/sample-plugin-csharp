<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <OutputType>Exe</OutputType>
    <ImplicitUsings>enable</ImplicitUsings>
    <PackageId>SamplePlugin</PackageId>
    <Version>0.0.1</Version>
    <Authors>Pete Kofod</Authors>
    <Company>NeuroSim</Company>
    <RepositoryUrl>https://github.com/neurosimio/sample-plugin-csharp</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Grpc.AspNetCore" Version="2.58.0" />
    <PackageReference Include="Grpc.AspNetCore.Server.Reflection" Version="2.71.0" />
    <PackageReference Include="SimSDK" Version="0.0.18" />
  </ItemGroup>


</Project>